{
  "name": "ldebrouwer/distance",
  "type": "library",
  "description": "Distance helps you calculate the distance between GPS coordinates, in vanilla PHP. Pure and simple.",
  "keywords": [
    "distance",
    "gps",
    "location",
    "coordinates",
    "php",
    "latitude",
    "longitude"
  ],
  "homepage": "https://github.com/ldebrouwer/distance",
  "license": "MIT",
  "authors": [
    {
      "name": "Luc De Brouwer",
      "email": "info@lucdebrouwer.nl",
      "homepage": "http://www.lucdebrouwer.nl"
    }
  ],
  "require": {
    "php": ">=8.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^13.0",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-phpunit": "^2.0",
    "roave/security-advisories": "dev-latest",
    "phpstan/phpstan-strict-rules": "^2.0",
    "squizlabs/php_codesniffer": "^4.0",
    "slevomat/coding-standard": "^8.27"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "autoload": {
    "psr-4": {
      "ldebrouwer\\Distance\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\ldebrouwer\\Distance\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
